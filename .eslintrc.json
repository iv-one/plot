{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "env": {
    "es2020": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "no-cond-assign": 0,
    "no-constant-condition": 0,
    "no-sparse-arrays": 0,
    "no-unexpected-multiline": 0,
    "no-unused-vars": ["error", {"ignoreRestSiblings": true}]
  }
}
