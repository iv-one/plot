{
  "name": "@observablehq/plot",
  "description": "A JavaScript library for exploratory data visualization.",
  "version": "0.6.4",
  "author": {
    "name": "Observable, Inc.",
    "url": "https://observablehq.com"
  },
  "license": "ISC",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/plot.umd.min.js",
  "unpkg": "dist/plot.umd.min.js",
  "exports": {
    "umd": "./dist/plot.umd.min.js",
    "default": "./src/index.js"
  },
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/observablehq/plot.git"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "test": "yarn test:mocha && yarn test:lint",
    "test:mocha": "mkdir -p test/output && mocha 'test/**/*-test.*' 'test/plot.js'",
    "test:lint": "eslint src test",
    "prepublishOnly": "rm -rf build dist && node scripts/readme-to-jsdoc.js && rollup -c",
    "postpublish": "git push && git push --tags",
    "dev": "vite"
  },
  "_moduleAliases": {
    "@observablehq/plot": "./src/index"
  },
  "sideEffects": [
    "./src/plot.js"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "canvas": "^2.0.0",
    "d3-geo-projection": "^4.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "htl": "^0.3.0",
    "js-beautify": "1",
    "jsdom": "^21.0.0",
    "mkdirp": "^1.0.4",
    "mocha": "^10.0.0",
    "module-alias": "^2.0.0",
    "prettier": "^2.7.1",
    "rollup": "^3.7.0",
    "topojson-client": "^3.1.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "interval-tree-1d": "^1.0.0",
    "isoformat": "^0.2.0"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
